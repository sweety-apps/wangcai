<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			xmlns:tools="http://schemas.android.com/tools"
			xmlns:wangcai = "http://schemas.android.com/apk/res/com.example.wangcai"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:orientation="vertical">
	<com.example.wangcai.ctrls.TitleCtrl 
		android:id="@+id/extract_alipay"
		android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<LinearLayout 
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
			<ImageView
				android:id="@+id/extract_logo"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" 	 />
			<TextView 
				android:id="@+id/extract_item_text"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			/> 
			<TextView 
				android:id="@+id/extract_item_sub_text"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			/> 
	</LinearLayout>
	<com.example.wangcai.ctrls.ExtractLineCtrl
		android:id="@+id/line1"
		android:layout_marginTop="20dp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<com.example.wangcai.ctrls.ExtractLineCtrl 
		android:id="@+id/line2"
		android:layout_marginTop="20dp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content" />
	<com.example.wangcai.ctrls.ExtractLineCtrl 
		android:id="@+id/line3"
		android:layout_marginTop="20dp"
		android:layout_width="match_parent"
		android:layout_height="wrap_content" />

	<TextView 
		android:layout_marginTop="20dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="@string/extract_label"
	/> 
	<LinearLayout 
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:orientation="horizontal">
		<com.example.wangcai.ctrls.ExtractPrieceCtrl 
			android:id="@+id/grid1"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
		<com.example.wangcai.ctrls.ExtractPrieceCtrl 
			android:id="@+id/grid2"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
		<com.example.wangcai.ctrls.ExtractPrieceCtrl 
			android:id="@+id/grid3"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
	</LinearLayout>
</LinearLayout>
